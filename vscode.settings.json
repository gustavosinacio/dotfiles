{
  "window.commandCenter": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-mocha",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrainsMono Nerd Font, Ink Free",
  "editor.tabSize": 2,
  "editor.renderWhitespace": "trailing",
  "files.exclude": {
    ".idea": true
  },
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.leader": " ",
  "vim.commandLineModeKeyBindingsNonRecursive": [],
  "vim.insertModeKeyBindingsNonRecursive": [],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Esc>"],
      "after": [":nohlsearch"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "x"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["\\"],
      "commands": ["workbench.explorer.fileView.focus"]
    },
    {
      "before": ["<leader>", "s", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "s", "g"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["tab"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["s-tab"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["editor.action.marker.this"]
    },
    {
      "before": ["<leader>", "k", "s"],
      "commands": ["workbench.action.openGlobalKeybindings"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["workbench.action.openEditorAtIndex1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["workbench.action.openEditorAtIndex2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["workbench.action.openEditorAtIndex3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["workbench.action.openEditorAtIndex4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["workbench.action.openEditorAtIndex5"]
    },
    {
      "before": ["<leader>", "6"],
      "commands": ["workbench.action.openEditorAtIndex6"]
    },
    {
      "before": ["<leader>", "7"],
      "commands": ["workbench.action.openEditorAtIndex7"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    }
  ],

  "vim.handleKeys": {
    "<C-b>": false,
    "<C-f>": false,
    "<C-y>": false,
    "<C-e>": false
    // "<C-v>": false,
    // "<C-s>": false,
    // "<C-z>": false,
    // "<C-p>": false,
    // "<C-q>": false,
    // "<C-n>": false,
    // "<C-x>": false,
    // "<C-a>": false,
  },
  "vim.operatorPendingModeKeyBindingsNonRecursive": [],
  "vim.visualModeKeyBindingsNonRecursive": [],
  "vim.vimrc.enable": false,
  "vim.highlightedyank.color": "#00aa0060",
  "vim.hlsearch": true,
  "vim.enableNeovim": true,
  "vim.neovimConfigPath": "~/.config/nvim/init.lua",
  "git.openRepositoryInParentFolders": "always",
  "editor.lineNumbers": "relative",
  "[snippets]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
